#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
using namespace std;

int dx[] = { -2,-1,1,2,2,1,-1,-2 };
int dy[] = { -1 - 2,-2,-1,1,2,2,1 };


int main()
{
	string str;
	cin >> str;
	int row = str[1] - '0'; // '1' -> 1 변환
	int col = str[0] - 'a' + 1; // 'a'-> 1 변환
	cout << row << ' ' << col << endl;
}
